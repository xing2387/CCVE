<script>
var t="li1|li2".split('|');
function showhide(name)
{
	for(var i=0;i<t.length;i++)
	{
		document.getElementById('div_'+t[i]).className="hide";
		document.getElementById(t[i]).className="";
	}
	document.getElementById('div_'+name).className="";
	document.getElementById(name).className="tabso1";
	
}
</script> 
